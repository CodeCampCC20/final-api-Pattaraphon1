generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Doctor {
  id              Int    @id @default(autoincrement())
  username        String @unique
  password        String
  confirmPassword String
  specialization  String
}

model User {
  id              Int    @id @default(autoincrement())
  username        String @unique
  password        String
  confirmPassword String
}

model HealthRecord {
  in    Int      @id @default(autoincrement())
  type  String
  value String
  date  DateTime
}

model DoctorNote {
  id       Int      @id @default(autoincrement())
  note     String
  createAt DateTime
}
